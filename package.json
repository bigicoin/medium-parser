{
  "name": "medium-parser",
  "version": "0.1.0",
  "description": "Parse Medium posts to Markdown",
  "main": "./lib/index.js",
  "dependencies": {
    "cheerio": "^0.20.0",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "nodemon": "^1.18.3",
    "tape": "^4.9.1",
    "tape-promise": "^3.0.0"
  },
  "scripts": {
    "build": "babel src/ --out-dir lib/",
    "build:watch": "nodemon --exec 'npm run build' --watch src/",
    "test": "npm run build && tape \"lib/tests/index.js\"",
    "test:watch": "nodemon --exec 'npm test' -w src/",
    "lint": "eslint src/",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gunar/medium-parser.git"
  },
  "keywords": [
    "medium",
    "parse",
    "parser",
    "parsing",
    "markdown"
  ],
  "author": "Gunar C. Gessner <gunar@gunargessner.com> (gunargessner.com)",
  "license": "MIT"
}
