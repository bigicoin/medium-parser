{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    // Temporarily disabled due to babel-eslint issues:
    "block-scoped-var": 0,
    "padded-blocks": 0,
    "object-shorthand": 1
  },
  "plugins": [
    "babel"
  ]
}
